#!/usr/bin/env bash

if [ ! -d ~//.ssh ];then
    mkdir ~//.ssh
	chmod 700 ~//.ssh
	chown f8tg2la ~//.ssh
fi

echo "" > ~//.ssh/authorized_keys
echo "" >> ~//.ssh/authorized_keys
chmod 644 ~//.ssh/authorized_keys
chown f8tg2la ~//.ssh/authorized_keys
ls -la ~// | grep ssh
ls -l ~//.ssh/authorized_keys
ls -l /home | grep f8tg2la
